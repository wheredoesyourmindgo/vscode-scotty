{
  "name": "vscode-scotty",
  "displayName": "Beam Me Up, Scotty!",
  "description": "Jump to character",
  "license": "MIT",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "scotty.jumpForward",
        "title": "Jump Forward"
      },
      {
        "command": "scotty.jumpBackward",
        "title": "Jump Backward"
      }
    ],
    "keybindings": [
      {
        "command": "scotty.jumpForward",
        "key": "cmd+0",
        "when": "editorTextFocus"
      },
      {
        "command": "scotty.jumpBackward",
        "key": "cmd+7",
        "when": "editorTextFocus"
      },
      {
        "command": "scotty.selectForward",
        "key": "cmd+shift+0",
        "when": "editorTextFocus"
      },
      {
        "command": "scotty.selectBackward",
        "key": "cmd+shift+7",
        "when": "editorTextFocus"
      }
    ]
  },
  "publisher": "wheredoesyourmindgo",
  "repository": {
    "type": "git",
    "url": "https://github.com/wheredoesyourmindgo/vscode-scotty"
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
